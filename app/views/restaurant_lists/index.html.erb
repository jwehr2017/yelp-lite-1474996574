<div class="page-header">
  <h1>
    Restaurant lists
    <a href="/restaurant_lists/new" class="btn btn-lg btn-success">New Restaurant list</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Restaurant name</th>
        <th>Address</th>
        <th>Cover photo</th>
        <th>Actions</th>
      </tr>

      <% @restaurant_lists.each do |restaurant_list| %>
      <tr>
        <td><%= restaurant_list.restaurant_name %></td>
        <td><%= restaurant_list.address %></td>
        <td><%= restaurant_list.cover_photo %></td>
        <td>
          <a href="/restaurant_lists/<%= restaurant_list.id %>" class="btn btn-primary">Show</a>
          <a href="/restaurant_lists/<%= restaurant_list.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_restaurant_list/<%= restaurant_list.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
